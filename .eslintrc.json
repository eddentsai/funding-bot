{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "rules": {
    "max-len": "off",
    "class-methods-use-this": "off",
    "indent": ["error", 4, {
      "SwitchCase": 1,
      "VariableDeclarator": 1,
      "outerIIFEBody": 1,
      "FunctionDeclaration": {
        "parameters": 1,
        "body": 1
      },
      "FunctionExpression": {
        "parameters": 1,
        "body": 1
      }
    }]
  }
}
